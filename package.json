{
  "name": "RChain-Status-Dapp",
  "version": "1.0.1",
  "description": "Build your first RChain dapp",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --format unix --ignore-pattern node_modules/ *.js sigui/*.js",
    "typecheck": "flow check",
    "fresh": "rm -rf .rnode/rspace .rnode/casper-block-store && rnode run --data_dir .rnode",
    "deploy-contract": "rnode deploy statusInsecure.rho && rnode deploy status.rho && rnode propose",
    "start": "node main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JoshOrndorff/Rchain-Status"
  },
  "author": "Joshy Orndorff",
  "license": "Unlicense",
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "flow-bin": "^0.80.0",
    "flow-interfaces-chrome": "^0.5.1"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.3.0",
    "express": "^4.16.3",
    "grpc": "^1.14.1",
    "rchain-api": "github:JoshOrndorff/RChain-API#f2fc0c0b9a488a44a9916f72bdafa541fd98ed69"
  }
}
