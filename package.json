{
  "name": "rchain-status",
  "version": "1.1.0",
  "description": "Build your first RChain dapp",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --format unix --ignore-pattern node_modules/ *.js sigui/*.js",
    "typecheck": "flow check",
    "deploy-contract": "rnode deploy --phlo-limit 1000000 --phlo-price 1 --from 0x01 --nonce 1 status.rho && rnode propose",
    "start": "node main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rchain-community/Rchain-Status"
  },
  "contributors": ["Dan Connolly", "Joshy Orndorff"],
  "license": "Unlicense",
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0"
  },
  "dependencies": {
    "docopt": "^0.6.2"
  }
}
